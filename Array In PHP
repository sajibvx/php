<?

//Indexing Array

$inDex = array('Name', 'City', 'House', 'Gender');

//Associative Array

$associateArr= array ('Name'=>'Sajib', 'Id'=>'232', 'Class'=>'10');

var_dump($inDex);
print_r ($associateArr);

//MuntiDImensional Array

$univerSity = [
    'Batch' => [
        'AA' => [
            'Name' => 'S Khan',
            'aHE' => ''
        ],
        'bb' => [
            'hELLO',
            'mELLO'
            ]
    ]
];

echo $univerSity;









?>
